{
  "backend": {
    "_target_": "cotlab.backends.VLLMBackend",
    "tensor_parallel_size": 1,
    "dtype": "bfloat16",
    "trust_remote_code": true,
    "max_model_len": 4096
  },
  "model": {
    "name": "allenai/Olmo-3.1-32B-Think",
    "variant": "32b",
    "max_new_tokens": 2048,
    "temperature": 0.7,
    "top_p": 0.9
  },
  "prompt": {
    "_target_": "cotlab.prompts.RadiologyPromptStrategy",
    "name": "radiology"
  },
  "dataset": {
    "_target_": "cotlab.datasets.RadiologyDataset",
    "name": "radiology",
    "path": "data/Radiology_Synthetic_Data.csv",
    "text_column": "Synthetic",
    "label_column": "Flag"
  },
  "experiment": {
    "_target_": "cotlab.experiments.RadiologyExperiment",
    "name": "radiology",
    "description": "Pathological fracture detection from radiology reports"
  },
  "seed": 42,
  "verbose": true,
  "dry_run": false
}